{
  "name": "legendary",
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/c/:path*",
      "destination": "/collection/:path*",
      "permanent": true
    },
    {
      "source": "/a/:path*",
      "destination": "/auction/:path*",
      "permanent": true
    },
    {
      "source": "/ai/:path*",
      "destination": "/ai-recommend/:path*",
      "permanent": true
    },
    {
      "source": "/cm/:path*",
      "destination": "/community/:path*",
      "permanent": true
    }
  ],
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ]
} 